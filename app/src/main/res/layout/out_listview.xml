<?xml version="1.0" encoding="UTF-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    >

    <ImageView
        android:id="@+id/out_iv1"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:src="@drawable/list_icon"
        />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="编号"
        android:id="@+id/out_listview_order_label"
        android:layout_toRightOf="@id/out_iv1"
        android:textSize="18sp"
        />
    <TextView
        android:id="@+id/out_tv0"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="18sp"
        android:layout_marginLeft="20dp"
        android:layout_toRightOf="@id/out_listview_order_label"
        >
    </TextView>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="类型"
        android:id="@+id/out_listview_symbol_label"
        android:layout_below="@id/out_listview_order_label"
        android:layout_toRightOf="@id/out_iv1"
        android:textSize="18sp"
        />
    <TextView
        android:id="@+id/out_tv1"
        android:layout_marginLeft="20dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="18sp"
        android:layout_below="@id/out_listview_order_label"
        android:layout_toRightOf="@id/out_listview_symbol_label"
        >
    </TextView>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="时间"
        android:id="@+id/out_listview_time_label"
        android:layout_below="@id/out_listview_symbol_label"
        android:layout_toRightOf="@id/out_iv1"
        android:textSize="18sp"
        />
    <TextView
        android:id="@+id/out_tv2"
        android:layout_marginLeft="20dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="18sp"
        android:layout_below="@id/out_listview_symbol_label"
        android:layout_toRightOf="@id/out_listview_time_label"
        >
    </TextView>
</RelativeLayout>